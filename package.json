{
  "name": "eventiCRO",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "pretest": "node ./scripts/check-api-base-url.mjs",
    "test": "pnpm run test:integration && pnpm run test:e2e",
    "test:integration": "schemathesis run --checks all --rate-limit=50 --hypothesis-max-examples=50 --base-url=$API_BASE_URL $API_BASE_URL/openapi.json",
    "test:e2e": "playwright test",
    "build:ui": "vite build",
    "build:sw": "workbox injectManifest",
    "build": "pnpm run build:ui && pnpm run build:sw"
  },
  "dependencies": {
    "bullmq": "^4.0.0",
    "hono": "^4.0.0",
    "yaml": "^2.0.0",
    "postgres": "^3.3.5",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "vite": "^5.0.0",
    "workbox-cli": "^7.0.0"
  }
}
